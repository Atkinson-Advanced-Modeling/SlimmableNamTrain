{
    "_notes": [
        "Corresponding 'non-adjustable' WaveNet model"
    ],
    "net": {
        "name": "WaveNet",
        "config": {
            "layers_configs": [
                {
                    "condition_size": 1,
                    "input_size": 1,
                    "channels": 8,
                    "head_size": 1,
                    "kernel_size": 6,
                    "dilations": [
                        1,
                        4,
                        16,
                        64,
                        256,
                        1,
                        4,
                        16,
                        64,
                        256,
                        1,
                        4,
                        16,
                        64,
                        256
                    ],
                    "activation": "Tanh",
                    "gated": false,
                    "head_bias": true
                }
            ],
            "head_scale": 0.01
        }
    },
    "loss": {
        "mrstft_weight": 2.0e-4
    },
    "optimizer": {
        "lr": 0.004
    },
    "lr_scheduler": {
        "class": "ExponentialLR",
        "kwargs": {
            "gamma": 0.994
        }
    }
}