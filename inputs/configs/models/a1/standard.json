{
    "_notes": [
        "'Standard WaveNet' as of October 2025"
    ],
    "net": {
        "name": "WaveNet",
        "config": {
            "layers_configs": [
                {
                    "condition_size": 1,
                    "input_size": 1,
                    "channels": 16,
                    "head_size": 8,
                    "kernel_size": 3,
                    "dilations": [
                        1,
                        2,
                        4,
                        8,
                        16,
                        32,
                        64,
                        128,
                        256,
                        512
                    ],
                    "activation": "Tanh",
                    "gated": false,
                    "head_bias": false
                },
                {
                    "condition_size": 1,
                    "input_size": 16,
                    "channels": 8,
                    "head_size": 1,
                    "kernel_size": 3,
                    "dilations": [
                        1,
                        2,
                        4,
                        8,
                        16,
                        32,
                        64,
                        128,
                        256,
                        512
                    ],
                    "activation": "Tanh",
                    "gated": false,
                    "head_bias": true
                }
            ],
            "head_scale": 0.02
        }
    },
    "optimizer": {
        "lr": 0.004
    },
    "loss": {
        "val_loss": "esr",
        "mrstft_weight": 0.0002
    },
    "lr_scheduler": {
        "class": "ExponentialLR",
        "kwargs": {
            "gamma": 0.993
        }
    }
}